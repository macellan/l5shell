{
    "name": "macellan/l5shell",
    "type": "library",
    "description": "L5shell - Add shell command wrapper to Laravel 5.*",
    "homepage": "http://github.com/macellan/l5shell",
    "keywords": ["laravel", "shell", "exec", "macellan"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "2.0.*",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Netson\\L4shell": "src/"
        }
    },
    "minimum-stability": "stable"
}
