{
    "name": "mertyildiran/l5shell",
    "type": "library",
    "description": "Another fork of L4shell - Add shell command wrapper to Laravel 4.*",
    "homepage": "https://github.com/bcalik/l4shell",
    "keywords": ["laravel", "shell", "exec"],
    "license": "MIT",
    "version": "1.3.0",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "2.0.*",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Netson\\L4shell": "src/"
        }
    },
    "minimum-stability": "stable"
}
